<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abhi.listview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Opening_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Length" />
        <activity android:name=".Weight" />
        <activity android:name=".Temperature" />
        <activity android:name=".Velocity" />
        <activity android:name=".Mass" />
        <activity android:name=".Pressure" />
        <activity android:name=".Force" />
        <activity android:name=".Area" />
        <activity android:name=".Volume" />
        <activity android:name=".Time" />
        <activity android:name=".Energy" />

    </application>

</manifest>